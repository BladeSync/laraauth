{
    "name": "bladesync/laraauth",
    "description": "A simple, built-in authentication package for Laravel 11+.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Hanzla",
            "email": "ahmadhanzla02@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "BladeSync\\Laraauth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeSync\\Laraauth\\Providers\\AuthPackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}